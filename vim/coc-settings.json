{
  "languageserver": {
    "rust": {
      "command": "rust-analyzer",
      "filetypes": ["rust"],
      "rootPatterns": ["Cargo.toml"]
    },
    "clangd": {
      "command": "clangd",
      "rootPatterns": ["compile_flags.txt", "compile_commands.json", "cmake-build-debug/compile_commands.json"],
      "filetypes": ["c", "cc", "cpp", "c++", "objc", "objcpp"]
    }
  },
  "cSpell.userWords": [
    "Alessandro",
    "Nucleo",
    "Pezzato",
    "alepez",
    "btreemap",
    "chrono",
    "devand",
    "dotenv",
    "fieldset",
    "gdbserver",
    "mqtt",
    "myhome",
    "onchange",
    "onclick",
    "oninput",
    "openocd",
    "pluggit",
    "readonly"
  ],
  "cSpell.enabled": false,
  "codeLens.enable": true,
  "diagnostic.virtualText": true,
  "git.addGBlameToBufferVar": true,
  "git.addGBlameToVirtualText": true
}
